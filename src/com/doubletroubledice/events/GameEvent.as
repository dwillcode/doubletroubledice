package com.doubletroubledice.events {	import flash.events.Event;		public class GameEvent extends Event	{		public static const DOUBLES_ROLLED : String = "doublesRolled";		public static const DOUBLE_JOKERS_ROLLED : String = "doublesJokersRolled";		public static const JOKER_ROLLED : String = "jokerRolled";		public static const DOUBLE_SIXES : String = "doubleSixes";		public static const GAME_OVER : String = "gameOver";		public static const PLAY_AGAIN : String = "playAgain";		public static const MILESTONE_ROLL : String = "mileStoneRoll";		public static const STREAK_MILESTONE_REACHED : String = "streakMileStoneReached";		public static const BONUS_POINTS : String = "bonusPoints";		public static const MULT_IS_TWO	: String = "multiplyer2";		public static const MULT_IS_THREE : String = "multiplyer3";		public static const START : String = "startGame";				private var _data : Object;				public function GameEvent(type:String, bubbles:Boolean = false, cancelable:Boolean = false, data : Object = null) 		{			_data = data;			super(type);		}				public function get data() : Object { return _data; }			}	}