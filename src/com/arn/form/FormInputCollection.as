package com.arn.form{   	import com.arn.collections.ICollection;	import com.arn.iterators.IIterator;    	import com.arn.iterators.ArrayIterator; 		public class FormInputCollection implements ICollection	{       	  private var _data:Array;       	  	  public function FormInputCollection() 	  {          _data = new Array();       }	  	        public function addElement( value:FormInput ) : void	  {          _data.push( value );       }	  	        public function iterator( type:String = null ) : IIterator	  {          return new ArrayIterator( _data );       }    } }